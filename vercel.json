{
  "version": 2,
  "builds": [
    {
      "src": "backend/**/*",
      "use": "@vercel/node"
    },
    {
      "src": "frontend2/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/$1" },
    { "src": "/(.*)", "dest": "frontend2/$1" }
  ]
}