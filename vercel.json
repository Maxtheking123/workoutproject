{
  "version": 2,
  "builds": [
    {
      "src": "frontend2/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend2/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "installCommand": "npm install --force"
  }
}