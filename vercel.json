{
  "version": 2,
  "builds": [
    {
      "src": "frontend2/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend2/build"
      }
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}